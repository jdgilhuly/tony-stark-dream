{
  "name": "@jarvis/cli",
  "version": "0.1.0",
  "description": "JARVIS CLI - Terminal interface for JARVIS AI assistant",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "jarvis": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsx src/index.tsx",
    "dev:watch": "tsx watch src/index.tsx",
    "start": "node dist/index.js",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@jarvis/core": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "conf": "^12.0.0",
    "figures": "^6.0.1",
    "ink": "^4.4.1",
    "ink-big-text": "^2.0.0",
    "ink-gradient": "^3.0.0",
    "ink-link": "^3.0.0",
    "ink-spinner": "^5.0.0",
    "ink-table": "^3.1.0",
    "ink-text-input": "^5.0.1",
    "node-microphone": "^0.1.1",
    "open": "^10.0.3",
    "ora": "^8.0.1",
    "react": "^18.2.0",
    "speaker": "^0.5.5",
    "ws": "^8.16.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/ws": "^8.5.10",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
